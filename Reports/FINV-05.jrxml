	<queryString><![CDATA[select docnum,docdate, c.custmrname, g.frwhscode,w.dscription as frwhs,wt.dscription as wtype1, 
wt1.dscription as wtype2, gd.towhscode,w1.dscription as towhs,gd.itemcode,
gd.quantity, i.dscription as itemname,ip.price, ip.pricecode, ioi.dscription as iteminfo, ioi.optdtlcode,g.comments,
(gd.quantity*ip.price) as jmlh
from goodstransfer g 
inner join goodstransferdetail1 gd on g.docentry=gd.docentry 
inner join warehouse w on g.frwhscode=w.whscode 
inner join warehouse w1 on w1.whscode=gd.towhscode 
inner join item i on i.itemcode=gd.itemcode 
inner join customer c on c.custmrcode=g.custmrcode 
inner join itemprice ip on ip.itemcode=gd.itemcode  
inner join itemoptdetail1 iod1 on iod1.ownercode=gd.itemcode and iod1.optionalcode='01' and iod1.optdtlcode=case when $P{brand}='' then iod1.optdtlcode else $P{brand} end
inner join itemoptionalinfo ioi on ioi.optionalcode=iod1.optionalcode and ioi.optdtlcode=iod1.optdtlcode 
inner join warehousetype wt on wt.whstype = w.whstype 
inner join warehousetype wt1 on wt1.whstype = w1.whstype
where
g.frwhscode=$P{whsFr} and g.towhscode=$P{whsTo} and 
g.docdate between $P{datefr} and $P{dateto}
and ip.pricecode between $P{pricefr} and $P{priceto}
group by itemcode]]></queryString>

