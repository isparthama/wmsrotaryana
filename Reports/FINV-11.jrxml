	<queryString><![CDATA[select
a.docnum,a.itemcode,a.dscription,a.quantity,a.uom1,a.uom2,a.uom3, a.baseon,a.sortno,
            case when a.baseon='Box' then a.boxqty else 0 end as boxqty,
            case when a.baseon='Inr' then a.inrqty else 0 end as inrqty,
            case when a.baseon='Pcs' then a.pcsqty else 0 end as pcsqty,
            case when a.baseon='Box' then a.boxqty*a.uom3 else
            case when a.baseon='Inr' then a.inrqty*a.uom2 else a.pcsqty end end as ttlpcs,
            a.boxqty*a.uom3+a.inrqty*a.uom2+a.pcsqty as ttlpcs2,
            a.boxqty  as boxqty2,
            a.inrqty  as inrqty2,
            a.pcsqty  as pcsqty2,a.npo,a.brt,p.cmpnyname,p.address,a.nmout,a.addr,a.kdbrand,a.nmbrand,
(a.boxqty*a.uom3+a.inrqty*a.uom2+a.pcsqty)*a.brt as totbrt,a.vol
from
(
select
a.docnum,a.itemcode,a.dscription,a.quantity,a.uom1,a.uom2,a.uom3, a.baseon,a.sortno,
            a.boxqty,a.sisabox,
case when mod(a.sisabox,a.uom2)=0 then a.sisabox/a.uom2 else
case when a.sisabox/a.uom2 <1 then 0 else floor(a.sisabox/a.uom2) end end as inrqty,
a.quantity-(a.boxqty*a.uom3+(case when mod(a.sisabox,a.uom2)=0 then a.sisabox/a.uom2 else
case when a.sisabox/a.uom2 <1 then 0 else FLOOR(a.sisabox/a.uom2) end end)*a.uom2) as pcsqty,
a.npo,a.brt,a.nmout,a.addr,a.kdbrand,a.nmbrand,a.vol
from
(
select   d.docnum,d1.itemcode,i.dscription,d1.quantity,i.uom1,i.uom2,i.uom3, pu.baseon,pu.sortno,
case when d1.quantity<i.uom3 or i.uom3=0 or i.uom3 is null then 0 else
case when mod(d1.quantity,i.uom3)=0 then round(d1.quantity/i.uom3,0) else floor(d1.quantity/i.uom3) end end as boxqty,
case when i.uom3=0 or i.uom3 is null then d1.quantity else mod(d1.quantity,i.uom3) end  as sisabox
,po.docNum as npo,i.weight1 as brt,a.dscription as nmout,a.address as addr,iod.optDtlCode as kdbrand,
iof.dscription as nmbrand,(i.length*i.width*i.height) as vol
from deliverydetail1 d1
inner join delivery d on d.docentry=d1.docentry
inner join address a on a.ownerCode=d.custmrCode and a.addrTypeCode='02'
inner join packlistno p on p.docnum=d.docnum and p.emplcode=$P{emplcode}
inner join item i on i.itemcode=d1.itemcode
inner join itemoptdetail1 iod on iod.ownerCode=d1.itemcode and iod.optionalCode='01'
inner join itemoptionalinfo iof on iod.optDtlCode=iof.optDtlCode
and iof.optionalCode='01' 
left join purchaseorder po on d1.baseDocNum=po.ref2
cross join packunit pu
/*where d1.itemcode='C110183'*/
) as a
) as a,preference p
where (case when a.baseon='Box' then a.boxqty*a.uom3 else
            case when a.baseon='Inr' then a.inrqty*a.uom2 else a.pcsqty end end ) <> 0
order by a.sortno,a.itemcode]]></queryString>

