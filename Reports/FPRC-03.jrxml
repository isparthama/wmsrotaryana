	<queryString><![CDATA[select po.Docnum,po.docStatus,po.docDate,po.docDueDate,po.WhsCode,po.priceCode,
s.suppCode,s.suppName,a.address,a.city,
po.taxPercent,po.totalAmount,po.discAmount,po.bfrTaxAmount,po.taxAmount,po.aftTaxAmount,
po.ref1,po.ref2,po.comments,
po1.itemcode,po1.quantity,i.dscription as itemName,po1.price,po1.linetotal
from purchasereturn po
inner join purchasereturndetail1 po1 on po.docentry=po1.docentry
left join item i on i.itemcode=po1.itemcode
inner join supplier s on po.custmrCode=s.suppCode 
left join address a on a.objType=s.objType and s.suppCode=a.ownerCode and a.addrTypeCode='03'
where 
po.custmrCode between $P{suppCodeFr} and $P{suppCodeTo} AND
po.docDate between $P{docDateFr} and $P{docDateTo} AND
po.docNum between $P{docNumFr} and $P{docNumTo}]]></queryString>

