	<queryString><![CDATA[SELECT 
invoice.`docNum` AS invoice_docNum, 
invoice.`docDate` AS invoice_docDate, 
invoice.`docDueDate` AS invoice_docDueDate, 
invoice.`taxPercent` AS invoice_taxPercent, 
invoice.`totalAmount` AS invoice_totalAmount, 
invoice.`discAmount` AS invoice_discAmount, 
invoice.`bfrTaxAmount` AS invoice_bfrTaxAmount, 
invoice.`taxAmount` AS invoice_taxAmount, 
invoice.`aftTaxAmount` AS invoice_aftTaxAmount, 
invoice.`discSumP1` AS invoice_discSumP1, 
invoice.`discSumP2` AS invoice_discSumP2, 
invoice.`discSumP3` AS invoice_discSumP3, 
invoice.`ref2` AS invoice_ref2, 
invoice.`whsCode` AS invoice_whsCode, 
invoice.`docStatus` AS invoice_docStatus, 
invoice.`docEntry` AS invoice_docEntry, 
invoice.`termCode` AS invoice_termCode, 
invoice.`comments` AS comments,
invoicedetail1.`lineNum` AS invoicedetail1_lineNum, 
invoicedetail1.`itemCode` AS invoicedetail1_itemCode, 
invoicedetail1.`dscription` AS invoicedetail1_dscription, 
invoicedetail1.`quantity` AS invoicedetail1_quantity, 
invoicedetail1.`price` AS invoicedetail1_price, 
invoicedetail1.`discPrcnt3` AS invoicedetail1_discPrcnt3, 
invoicedetail1.`isItemBns` AS invoicedetail1_isItemBns, 
d1.baseDocNum AS invoicedetail1_baseDocNum, 
invoicedetail1.`visOrder` AS invoicedetail1_visOrder, 
invoicedetail1.`docEntry` AS invoicedetail1_docEntry, 
invoicedetail1.`discPrcnt1` AS invoicedetail1_discPrcnt1, 
invoicedetail1.`discPrcnt2` AS invoicedetail1_discPrcnt2, 
customer.`custmrCode` AS customer_custmrCode, 
customer.`custmrName` AS customer_custmrName, 
preference.`cmpnyName` AS preference_cmpnyName, 
preference.`address` AS preference_address, 
preference.`city` AS preference_city, 
preference.`phone1` AS preference_phone1, 
preference.`npwpNumber` AS preference_npwpNumber, 
item.`dscription` AS item_dscription, 
(
SELECT count( linenum )
FROM `invoicedetail1`
WHERE invoicedetail1.docEntry = invoice.docEntry and invoicedetail1.quantity>0
) AS ttlbrs, 
preference.`faSpv` AS preference_faSpv, 
address.`address` AS address_address, 
address.`city` AS address_city, 
address2.`address` AS address2_address,
address2.`city` AS address2_city,
salesman.`slPrsnName` AS salesman_slPrsnName, 
invoice.`npwpNo` AS npwp_npwpNumber,
npwp.npwpname as npwpname
FROM `invoice` invoice
INNER JOIN `invoicedetail1` invoicedetail1 ON invoicedetail1.`docEntry` = invoice.`docEntry` and invoicedetail1.quantity >0
INNER JOIN `customer` customer ON invoice.`custmrCode` = customer.`custmrCode`
LEFT join deliverydetail1 d1 on invoicedetail1.baseEntry=d1.docentry and invoicedetail1.baseLine=d1.lineNum
LEFT JOIN `npwp` npwp ON invoice.`custmrCode` = npwp.`ownerCode`
LEFT JOIN `address` address ON invoice.`custmrCode` = address.`ownerCode` and address.addrTypeCode='02'
LEFT JOIN `address` address2 ON npwp.`ownercode` = address2.`ownerCode` and address2.addrTypeCode='04'
LEFT JOIN `salesman` salesman ON invoice.`slPrsnCode` = salesman.`slPrsnCode`
LEFT JOIN `addrtype` addrtype ON address.`addrTypeCode` = addrtype.`addrTypeCode`
LEFT JOIN `item` item ON invoicedetail1.`itemCode` = item.`itemCode` 
, `preference` preference
WHERE invoice.custmrCode
BETWEEN $P{custmrCodeFr}
AND $P{custmrCodeTo}
AND invoice.docNum
BETWEEN $P{docNumFr}
AND $P{docNumTo}
AND invoice.docDate BETWEEN $P{docDateFr} AND $P{docDateTo} 

ORDER BY invoice.docnum, invoicedetail1.`linenum` ASC]]></queryString>

