inner join itempricehistory iph on iph.yop=year(cast($P{dateFr} as datetime)) and Mop=11 and iph.itemcode=d1.itemcode and iph.pricecode=(case when $P{priceCode}='' then 'HPD' else $P{priceCode} end)
group by d.custmrcode,d.slPrsnCode,iod.optionalCode,iod.optDtlCode,d1.itemcode
union all
select d.custmrcode,d.slPrsnCode,
csd.optionalCode as CustoptionalCode,csd.optDtlCode as CustoptDtlCode,
iod.optionalCode as ItemoptionalCode,iod.optDtlCode as ItemoptDtlCode,d1.itemcode,
0 as Jan,0 as Feb,0 as Mar,0 as Apr,0 as Mei,0 as Jun,
0 as Jul,0 as Ags,0 as Sep,0 as Okt,0 as Nov,sum(d1.quantity* 
(case when $P{unit}='1' then 1  else (case when $P{priceCode}='' then d1.price else iph.price end) end )
) as Des

from delivery d 
inner join 	deliverydetail1 d1 on d.docentry=d1.docentry and year(d.docdate)=year(cast($P{dateFr} as datetime)) and month(d.docdate)=12
		and cast(cast(d.docdate as char(11)) as datetime) between cast($P{dateFr} as datetime) and cast($P{dateTo} as datetime) and d.custmrcode between $P{custmrCodeFr} and $P{custmrCodeTo} and d.docstatus<>'B' and d1.isItemBns='N'
inner join custoptsettdetail1 csd on csd.ownerCode=d.custmrcode and csd.optionalCode=case when $P{custGroupBy}='' then '01' else $P{custGroupBy} end and csd.optDtlCode between $P{custGroupByFr} and $P{custGroupByTo}
inner join itemoptdetail1 iod on iod.ownerCode=d1.itemcode and iod.optionalCode=$P{optionalCode} and iod.optDtlCode between $P{optDtlCodeFr} and $P{optDtlCodeTo}
inner join itempricehistory iph on iph.yop=year(cast($P{dateFr} as datetime)) and Mop=12 and iph.itemcode=d1.itemcode and iph.pricecode=(case when $P{priceCode}='' then 'HPD' else $P{priceCode} end)
group by d.custmrcode,iod.optionalCode,iod.optDtlCode,d1.itemcode
) as x
inner join address a on a.ownercode=x.custmrcode and a.addrcode='000' and a.addrTypecode='02'
inner join itemoptsett ios on ios.optionalCode=x.itemOptionalCode
inner join itemoptionalinfo ioi on ioi.optionalCode=x.ItemoptionalCode and ioi.optDtlCode=x.itemOptDtlCode
inner join custoptsett cosett on cosett.optionalCode=custoptionalCode
inner join custoptinfo coi on coi.optionalCode=x.CustoptionalCode and coi.optDtlCode=x.custoptDtlCode
inner join item i on i.itemcode=x.itemcode
inner join salesman s on s.slprsncode=x.slprsncode and x.slprsncode=case when $P{salesman}="" then x.slprsncode else $P{salesman} end
cross join preference p
group by p.cmpnyId,p.cmpnyName,x.ItemoptionalCode,
	x.ItemoptDtlCode,x.slprsncode,rtrim(x.custmrcode),x.itemcode]]></queryString>

