	<queryString><![CDATA[select x.docdate,x.docnum,x.custmrcode, a.dscription as custmrname, a.address as custmraddress,
x.itemcode,x.itemname, sum(x.stm) as stm, sum(x.p3) as p3, sum(x.p3STM) as p3STM, sum(x.p3Value) as p3Value
from 
(
select d.docnum,d.docdate,
d.custmrcode,d1.itemcode, i.dscription as itemname, 
case when d1.isitembns='N' then d1.quantity else 0 end as stm,  
case when d1.isitembns='N' then (d1.quantity*iph.price) else 0 end as p3STM,
case when d1.isitembns='Y' then d1.quantity else 0 end as p3,
case when d1.isitembns='Y' then (d1.quantity*iph.price) else 0 end as p3Value
from deliverydetail1 d1 inner join delivery d on d1.docentry=d.docentry and   
d.docdate between $P{dtfr} and $P{dtto} and d.docstatus <> 'B' and d1.quantity > 0
inner join item i on i.itemcode=d1.itemcode
inner join itempricehistory iph on iph.itemcode=d1.itemcode and iph.yop=year(d.docdate) and iph.mop=month(d.docdate) and iph.pricecode=$P{price}
) as x
inner join address a on a.ownercode=x.custmrcode and a.addrtypecode='02'
Where exists (select itemcode from penetrationdetail1 pd inner join penetration p on pd.docentry=p.docentry where pd.itemcode=x.itemcode and p.docnum=$P{penet})

group by  x.custmrcode,x.docnum
order by x.custmrcode, x.docnum]]></queryString>

