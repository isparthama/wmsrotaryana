	<queryString><![CDATA[select  x.docnum,x.docdate ,x.custmrcode, a.dscription as custmrname, a.address as custmraddress,
it.optdtlcode as brand, io.dscription as brandname,
x.itemcode,x.itemname, sum(x.stm) as stm, sum(x.p3) as p3,
sum(x.stm)*case when ph.price is null then 0 else ph.price end as stmvalue, 
sum(x.p3)*case when ph.price is null then 0 else ph.price end as p3value ,
case when $P{sortByItem}='1' then x.itemcode else x.custmrcode end as sort1,
case when $P{sortByItem}='1' then x.custmrcode else x.itemcode end as sort2
from 
(
select d.docnum,d.docdate,
d.custmrcode,d1.itemcode, i.dscription as itemname, 
case when d1.isitembns='N' then d1.quantity else 0 end as stm,  
case when d1.isitembns='Y' then d1.quantity else 0 end as p3
from deliverydetail1 d1 inner join delivery d on d1.docentry=d.docentry and     
cast(cast(d.docdate as char(11)) as datetime) between cast($P{dtfr} as datetime) and cast($P{dtto} as datetime) and d.docstatus <> 'B' 
inner join item i on i.itemcode=d1.itemcode
inner join penetrationdetail1 pd on pd.itemcode=d1.itemcode
inner join penetration p on p.docentry=pd.docentry and p.docnum between $P{penfr} and $P{pento}
) as x
inner join address a on a.ownercode=x.custmrcode and a.addrcode='000' and a.addrtypecode='02'
inner join itemoptdetail1 it on it.optionalcode='01' and it.ownercode=x.itemcode 
		and it.optdtlcode between $P{brfr} and $P{brto}
inner join itemoptionalinfo io on io.optionalcode='01' and io.optdtlcode between $P{brfr} and $P{brto}
left join itempricehistory ph on ph.yop=year(x.docdate) and ph.mop=month (x.docdate) and ph.pricecode=$P{price} and ph.itemcode=x.itemcode
group by x.custmrcode, x.itemcode]]></queryString>

