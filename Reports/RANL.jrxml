	<queryString><![CDATA[select iod.optdtlcode, iof.dscription as brand, sl.slprsncode, sl.slprsnname, a.dscription as cname, a.address,d.custmrcode, count( distinct d.docnum) as docnum, sum(d1.quantity) as units,  sum(d1.quantity * iph.price) as value
from delivery d
inner join deliverydetail1 d1 on d.docentry=d1.docentry and d.docdate between $P{dateFr} and $P{dateTo} and d.docstatus<>'B' and d1.isitembns='N'
and d.slprsncode between $P{slsprsnFr} and $P{slsprsnTo}
inner join item i on i.itemcode=d1.itemcode
inner join address a on a.ownercode=d.custmrcode and a.addrtypecode='02'
inner join itempricehistory iph on iph.itemcode=d1.itemcode and iph.yop=year($P{dateFr}) and iph.mop=month($P{dateTo}) and iph.pricecode=$P{priceCode}
inner join itemoptdetail1 iod on iod.ownercode=d1.itemcode and iod.optionalcode=$P{optionalCode} and iod.optdtlcode between $P{optDtlCodeFr} and $P{optDtlCodeTo}
inner join itemoptionalinfo iof on iof.optionalcode=$P{optionalCode} and iof.optdtlcode=iod.optdtlcode
inner join salesman sl on sl.slprsncode=d.slprsncode
group by iod.optdtlcode, iof.dscription, sl.slprsncode, sl.slprsnname, a.dscription, a.address,d.custmrcode]]></queryString>

