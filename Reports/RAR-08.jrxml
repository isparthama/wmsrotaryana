	<queryString><![CDATA[select pd.batchentry, pd.comments, i.docnum, pd.registerdate, i.docdate, i.docduedate,co.dscription 
as grpdesc,cod.optionalcode as grpcode,cod.optdtlcode, i.slprsncode, i.custmrcode,c.custmrname,
a.address,pd3.entrytypecode, pd3.paytotal, pr.registerNo, pd.registerNo as LHPPno, pd.ref
from invoice i 
inner join ar_paymententrydetail3 pd3 on i.docnum=pd3.docnum and pd3.entrytypecode=$P{tranType}
inner join ar_paymententrydetail1 pd1 on pd1.docentry = pd3.docentry 
inner join ar_paymententry pd on pd.batchentry=pd1.bacthentry and pd1.docentry =pd3.docentry
inner join custoptsettdetail1 cod on cod.ownercode = i.custmrcode and cod.optionalcode=$P{grpOutlet} 
inner join custoptsett co on cod.optionalcode = co.optionalcode 
inner join address a on a.ownercode=i.custmrcode and a.addrtypecode='02' 
inner join customer c on c.custmrcode = i.custmrcode
left join ar_paymentregist pr on pr.batchEntry=pd.baseBatch
where
pd.registerdate between $P{docDateFr} and $P{docDateTo}
and cod.optdtlcode between $P{grpOutletDetailFr} and $P{grpOutletDetailTo}]]></queryString>

