	<queryString><![CDATA[select 
x.custmrcode,max(x.custmrName) as custmrName,max(x.addressName) as addressName,
sum(x.saldoAwl) as saldoAwl,sum(x.saldoAkh) as saldoAkh,sum(x.plafond) as plafond,
sum(x.plafond)-sum(x.saldoAkh) as sisaOrder,x1.Cash,x1.GiroPDC,x1.ACabDbt,x1.DNKorDbt ,x1.DNKorDb,
(select sum(docTotal) from ar_invoiceh where yop=$P{yop} and mop=$P{mop} and custmrcode=x.custmrcode  and bgnBalance=0) as penjBlnIni,
x2.tglDok,x2.noFak,
x2.spb,x2.tglFak,x2.registerNo,x2.noDok_note,
x2.mtsDbt,x2.mtsCdt,x2.sisaFak,
x2.tglJT,x2.visOrder

from
(
/*---- master customer ---------*/
select 
c.custmrcode,a.dscription as custmrName,a.address as addressName,
0 as saldoAwl,0 as saldoAkh,c.credLimit as plafond, c.balance as sisaorder,
null as tglDok,null as noFak,null as spb,null as tglFak,null as noDok_note,0 as mtsDbt,0 as mtsCdt,0 as sisaFak,null as tglJT
from
customer c inner join address a on a.ownercode=c.custmrcode and a.addrTypeCode='02'
	and c.custmrcode between $P{custmrCodeFr} and $P{custmrCodeTo}
union all
/*---- master Saldo Awal & Akhir  ---------*/
select 
c.custmrcode,null as custmrName,null as addressName,
sum(c.bgnBalance) as saldoAwl,sum(c.endBalance) as saldoAkh,0 as plafond, 0 as sisaorder,
null as tglDok,null as noFak,null as spb,null as tglFak,null as noDok_note,
0 as mtsDbt,
0 as mtsCdt,0 as sisaFak,
null as tglJT
from
ar_invoiceh c
where c.yop=$P{yop} and c.mop=$P{mop} and c.custmrcode between $P{custmrCodeFr} and $P{custmrCodeTo}
group by c.custmrcode
) as x
inner join 
(
select
c.custmrcode,
sum(c.Cash) as Cash, sum(c.GiroPDC) as GiroPDC,
sum(c.ACabDbt) as ACabDbt,sum(c.DNKorDbt) as DNKorDbt ,sum(c.DNKorDb) as DNKorDb
from
(
select p.custmrcode,
case when p1.entryTypeCode='01' then p1.paytotal else 0 end as Cash,
case when p1.entryTypeCode='03' then p1.paytotal else 0 end as GiroPDC,
case when p1.entryTypeCode in ('10') then p1.paytotal else 0 end as ACabDbt,
case when p1.entryTypeCode in ('06','08') then p1.paytotal else 0 end as DNKorDbt,
case when p1.entryTypeCode in ('04','05','07','09','11') then p1.paytotal else 0 end as DNKorDb ,
case when p1.entryTypeCode='03' then (select chckStatus from ar_giroregistrationh  where yop=$P{yop} and mop=$P{mop} and checknum=p1.checknum  limit 1) else 'O' end as chckStatus
from ar_invoicehdetail1 p1 
inner join ar_invoiceh p on 
		p1.yop=$P{yop} and p1.mop=$P{mop} and p.docnum=p1.docnum and 
		p.yop=$P{yop} and p.mop=$P{mop} and p.custmrcode between $P{custmrCodeFr} and $P{custmrCodeTo}
) as c
where c.chckStatus='O'
group by c.custmrcode
) as x1 on x.custmrcode=x1.custmrcode


inner join

/* ------------------------------------ */

(
select 
c.custmrcode,
null as tglDok,c.docnum as noFak,
(select o.docnum from invoicedetail1 i1 left join `order` o on i1.baseentry=o.docentry
 where i1.docentry=i.docentry limit 1) as spb,i.docdate as tglFak,null as registerNo,null as noDok_note,
c.bgnBalance+c.docTotal as mtsDbt,
0 as mtsCdt,c.endBalance as sisaFak,
i.docduedate as tglJT,1 as visOrder
from
ar_invoiceh c 
inner join invoice i on c.docnum = i.docnum
and c.yop=$P{yop} and c.mop=$P{mop} and  c.endBalance<>0 and
c.custmrcode between $P{custmrCodeFr} and $P{custmrCodeTo} 
group by c.custmrcode,c.docnum

union all
/* ------------------------*/
select
b.custmrcode,
b.tglDok,b.noFak,(select o.docnum from invoicedetail1 i1 left join `order` o on i1.baseentry=o.docentry
 where i1.docentry=i.docentry limit 1) as spb,b.tglFak,b.registerNo,b.ref as noDok_note,
ih.bgnBalance+ih.doctotal as mtsDbt,
b.mtsCdt as mtsCdt,b.sisaFak as sisaFak,
b.tglJT as tglJT,2 as visOrder

from
(
select
x1.docentry,max(x1.custmrCode) as custmrCode,
max(x1.tglDok) as tglDok,
sum(x1.noFak) as noFak,max(x1.tglFak) as tglFak,max(x1.registerNo) as registerNo ,max(x1.ref) as ref,
sum(x1.mtsDbt) as mtsDbt,sum(x1.mtsCdt) as mtsCdt,sum(x1.sisaFak) as sisaFak,max(x1.tglJT) as tglJT
from
(
select p1.docentry,p1.custmrCode,
p.registerDate as tglDok,
null as noFak,null as tglFak,p.registerNo ,p.ref,0 as mtsDbt,0 as mtsCdt,0 as sisaFak,null as tglJT
from
ar_paymententry p
inner join 	ar_paymententrydetail1 p1 on p.batchentry =p1.bacthEntry and 
		year(p.registerDate)=$P{yop} and month(p.registerDate)=$P{mop} and
		p1.custmrcode between $P{custmrCodeFr} and $P{custmrCodeTo}
union all

select 
a.docentry,a.custmrCode,
a.tglDok,
a.noFak,a.tglFak,a.registerNo ,a.ref,sum(a.mtsDbt) as mtsDbt,sum(a.mtsCdt) as mstCdt,sum(a.sisaFak) as  sisaFak,a.tglJT
from
(
select p2.docentry,null as custmrcode,
null as tglDok,
p2.docNum as noFak,p2.docdate as tglFak,0 as registerNo ,null as ref,0 as mtsDbt,
p3.payTotal as mtsCdt,0 as sisaFak,p2.docduedate as tglJT,p3.entryTypeCode,p3.baseDocnum,
case when p3.entryTypeCode='03' then 
( select chckStatus from ar_giroregistrationh where 
checkNum=p3.baseDocNum and
year(docdate)=$P{yop} and month(docdate)=$P{mop} limit 1) else 'O' end as chckStatus
from
ar_paymententrydetail2 p2
inner join ar_paymententrydetail3 p3 on p2.docentry =p3.docentry and p2.linenum=p3.baseline
and p3.entryTypeCode='03'
) as a
where a.chckStatus='O'
group by 
a.docentry,a.custmrCode,
a.tglDok,
a.noFak,a.tglFak,a.registerNo ,a.ref
) as x1
group by x1.docentry
having max(x1.custmrCode) is not null and max(x1.nofak) is not null
) as b
inner join invoice i on b.noFak=i.docnum
inner join ar_invoiceh ih on ih.docnum=b.noFak and ih.yop=$P{yop} and ih.mop=$P{mop}

union all
/* ------------------------------*/
select 
p1.custmrcode, p.registerDate as tglDok,p2.docnum as noFak,
(select o.docnum from invoicedetail1 i1 left join `order` o on i1.baseentry=o.docentry
 where i1.docentry=i.docentry limit 1) as spb,p2.docdate as tglFak,p.registerNo,p.ref as noDok_note,
0 as mtsDbt,p3.payTotal as mtsCdt ,0 as sisaFak,p2.docduedate as tglJT,3 as visOrder

from  ar_invoiceh ih  
inner join 	ar_paymententrydetail2 p2 on ih.docnum=p2.docnum and ih.yop=$P{yop} and ih.mop=$P{mop} and
		ih.custmrcode=$P{custmrCodeFr}
inner join ar_paymententrydetail3 p3 on p3.docentry=p2.docentry and p3.baseline=p2.linenum and p3.EntryTypeCode in ('01','02','03','04','05','07','09','11')
inner join 	ar_paymententrydetail1 p1 on p2.docentry=p1.docentry
inner join 	ar_paymententry p on p.batchEntry=p1.bacthentry and 
		p1.custmrcode=$P{custmrCodeFr}  

left join invoice i on i.docnum=p2.docnum
left join ar_giroregistrationh gh on gh.yop=$P{yop} and gh.mop=$P{mop} and gh.checknum=p3.basedocnum and 
		p3.EntryTypeCode='03' 
where 
case when p3.EntryTypeCode='03' then gh.chckStatus else 
case when year(p.registerdate)=ih.yop and month(p.registerdate)=ih.mop then 'C' else 'O' end end = 'C'

) as x2
on x.custmrcode=x2.custmrcode
group by 
x.custmrcode,x.tglDok,x.noFak,x.spb,x.tglFak,x.noDok_note,x.tglJT,x2.tglDok,x2.noFak,
x2.spb,x2.tglFak,x2.registerNo,x2.noDok_note,
x2.mtsDbt,x2.mtsCdt,x2.sisaFak,
x2.tglJT,x2.visOrder]]></queryString>

