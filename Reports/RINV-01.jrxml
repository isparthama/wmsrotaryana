	<queryString><![CDATA[SELECT
     goodsreceipt.`custmrCode` AS custmrCode,
     goodsreceipt.`BfrTaxAmount` AS BfrTaxAmount,
     goodsreceipt.`AftTaxAmount` AS AftTaxAmount,
     goodsreceipt.`docNum` AS gdocNum,
     customer.`custmrName` AS custmrName,
     goodsreceipt.`docDate` AS docDate,
     goodsreceipt.`ref2` AS ref2
FROM
     `goodsreceipt` goodsreceipt LEFT OUTER JOIN `customer` customer ON customer.`custmrCode` = goodsreceipt.`custmrCode`
WHERE goodsreceipt.docDate BETWEEN $P{docDateFr} AND $P{docDateTo} AND goodsreceipt.docNum BETWEEN $P{docNumFr} AND $P{docNumTo}
AND goodsreceipt.custmrCode BETWEEN $P{custmrCodeFr} AND $P{custmrCodeTo} AND goodsreceipt.tranType=$P{tranType}]]></queryString>

