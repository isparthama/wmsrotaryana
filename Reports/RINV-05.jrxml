	<queryString><![CDATA[select gt.docdate,gt.docnum,gt.comments,gt.frwhscode,wt.dscription as tipegdgfr,gt.towhscode,
wt1.dscription tipegdgto,sum(gt1.quantity) as unit, sum(gt1.quantity*iph.price) as jumlah,w1.dscription,
iod.optdtlcode as brand, iof.dscription as nmbrand,iph.pricecode
from goodstransfer gt 
inner join goodstransferdetail1 gt1 on gt.docentry=gt1.docentry
and gt.docdate between $P{datefr} and $P{dateto}  and gt.frwhscode=$P{whscode} and gt.towhscode=$P{whscodeto}
inner join itemoptdetail1 iod on gt1.itemcode=iod.ownercode and iod.optionalcode=01  and iod.optdtlcode=case when $P{brnd}='' then iod.optdtlcode else $P{brnd} end   
inner join itemoptionalinfo iof on iof.optionalcode=iod.optionalcode  and iof.optdtlcode=iod.optdtlcode
inner join itempricehistory iph on gt1.itemcode=iph.itemcode and iph.yop=year(gt.docdate) and iph.mop=month(gt.docdate)
and iph.pricecode=$P{prcode}
inner join warehouse w on gt.frwhscode=w.whscode and gt.frwhscode=$P{whscode}
inner join warehouse w1 on gt.towhscode=w1.whscode and gt.towhscode=$P{whscodeto}
inner join warehousetype wt on w.whstype=wt.whstype 
inner join warehousetype wt1 on w1.whstype=wt1.whstype
group by iod.optdtlcode, gt.docnum]]></queryString>

