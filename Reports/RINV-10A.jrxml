inner join itemoptdetail1 iod1 on iod1.ownercode=po1.itemcode and iod1.optionalcode='05' 
inner join itemoptionalinfo iof on iof.optionalcode=iod.optionalcode and iof.optdtlcode=iod.optdtlcode
