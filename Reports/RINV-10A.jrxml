from 
purchaseorder po inner join purchaseorderdetail1 po1 on po.docentry=po1.docentry and po.docdate between cast(cast($P{datefr} as char(11)) as datetime) 
and cast(cast($P{dateto} as char(11)) as datetime) and po.docstatus<>'B' and po.docnum between $P{docfr} and $P{docto}
