	<queryString><![CDATA[select p.cmpnyid, p.cmpnyname, po.docnum, po.docdate, po.ref1, po.ref2, po.comments,
iod.optdtlcode,io.dscription as optdesc, pod.itemcode,i.dscription as itemdesc, pod.quantity, pr1.quantity
as realqty
from
purchaseorder po 
inner join purchaseorderdetail1 pod on pod.docentry=po.docentry 
inner join purchasereceivedetail1 pr1 on pr1.baseentry=pod.docentry  
and pr1.baseline = pod.linenum inner join item i on i.itemcode = pod.itemcode 
inner join itemoptdetail1 iod on iod.ownercode=pod.itemcode and iod.optionalcode='01' 
inner join itemoptionalinfo io on io.optdtlcode=iod.optdtlcode and iod.optionalcode='01' 
cross join 
preference p
where docdate between $P{docDateFr} and $P{docDateTo}
order by docnum]]></queryString>

