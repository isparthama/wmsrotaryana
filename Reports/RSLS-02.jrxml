	<queryString><![CDATA[select
Yop,mop,today,docdatetxtku,docdatetxtku2,ordno,docnum,custmrcode,custmrname,whsCode,optionalcode,
opt1,opt2,docdate,docduedate,y.slPrsnCode,slPrsnName,pkpsts,taxAmount,docstatus,
jumlah,p1,p2,p3,jumlahkotor,taxReport,ppn,TotJmlKtr,Sort1,Sort2,pricecode,Sort,y.ref1,y.ref2,
case when docstatus<>'B' then jumlahHPDSAI else 0 end as jumlahHPDSAI,
case when docstatus<>'B' then jumlahHPDSAIPlusP3 else 0 end as jumlahHPDSAIPlusP3,
case when docstatus<>'B' and pricecode='HPD' then jumlah else 0 end as jumlahHPD,
case when docstatus<>'B' and pricecode='HPD' then p1 else 0 end as p1HPD,
case when docstatus<>'B' and pricecode='HPD' then p2 else 0 end as p2HPD,
case when docstatus<>'B' and pricecode='HPD' then p3 else 0 end as p3HPD,
case when docstatus<>'B' and pricecode='HPD' then jumlahkotor else 0 end as jumlahkotorHPD,
case when pkpsts='0' then 0 else 
	case when docstatus<>'B' and pricecode='HPD' then ppn else 0 end
	end as pkpHPD,
case when pkpsts='1' then 0 else 
	case when docstatus<>'B' and pricecode='HPD' then ppn else 0 end
	end as nonpkpHPD,
case when docstatus<>'B' and pricecode='HPD' then ppn else 0 end as ppnHPD,
case when docstatus<>'B' and pricecode='HPD' then jumlahkotor+ppn else 0 end as jumlahbersihHPD,

case when docstatus<>'B' and pricecode='HPDMOP' then jumlah else 0 end as jumlahMOP,
case when docstatus<>'B' and pricecode='HPDMOP' then p1 else 0 end as p1MOP,
case when docstatus<>'B' and pricecode='HPDMOP' then p2 else 0 end as p2MOP,
case when docstatus<>'B' and pricecode='HPDMOP' then p3 else 0 end as p3MOP,
case when docstatus<>'B' and pricecode='HPDMOP' then jumlahkotor else 0 end as jumlahkotorMOP,
case when pkpsts='0' then 0 else 
	case when docstatus<>'B' and pricecode='HPDMOP' then ppn else 0 end
	end as pkpMOP,
case when pkpsts='1' then 0 else 
	case when docstatus<>'B' and pricecode='HPDMOP' then ppn else 0 end
	end as nonpkpMOP,
case when docstatus<>'B' and pricecode='HPDMOP' then ppn else 0 end as ppnMOP,
case when docstatus<>'B' and pricecode='HPDMOP' then jumlahkotor+ppn else 0 end as jumlahbersihMOP,

case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then jumlah else 0 end as jumlahOth,
case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then p1 else 0 end as p1Oth,
case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then p2 else 0 end as p2Oth,
case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then p3 else 0 end as p3Oth,
case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then jumlahkotor else 0 end as jumlahkotorOth,
case when pkpsts='0' then 0 else 
	case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then ppn else 0 end
	end as pkpOth,
case when pkpsts='1' then 0 else 
	case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then ppn else 0 end
	end as nonpkpOth,
case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then ppn else 0 end as ppnOth,
case when docstatus<>'B' and not (pricecode in ('HPDMOP','HPD')) then jumlahkotor+ppn else 0 end as jumlahbersihOth


from
(
select year($P{docdate}) as Yop ,month($P{docdate}) as mop,day($P{docdate}) as today,
	cast(cast(x.docdate as char(11)) as datetime) as docdatetxtku,cast($P{docdate} as datetime)  as docdatetxtku2,
ordno,docnum,x.custmrcode,c.custmrname,x.whsCode,x.optionalcode,ios.dscription as opt1,ioi.dscription as opt2,x.docdate,
x.docduedate,x.slPrsnCode,x.pkpsts,x.taxAmount,x.docstatus,sum(jumlahHPDSAI) as jumlahHPDSAI,
sum(jumlahHPDSAIPlusP3) as jumlahHPDSAIPlusP3,
sum(jumlah) as jumlah,sum(p1) as p1, sum(p2) as p2,sum(p3) as p3,sum(jumlahkotor) as jumlahkotor,x.taxReport,
round(x.taxReport*(sum(jumlahkotor)/TotJmlKtr),0) as ppn,x.ref1,x.ref2,
TotJmlKtr,
case when $P{slPrsnCode}='' then x.slprsncode else cast(x.docdate as char(11)) end as Sort,
case when $P{whscode}='' then 'All Warehouse' else x.whscode end as Sort1,
case when $P{optionalCode}='' then 'All Group' else x.optdtlcode end as Sort2,
x.pricecode	
from
( 
select o.docnum as ordno,d.docnum,d.docdate,d.docduedate,d.slPrsnCode,optionalcode,
d.custmrcode,d1.whscode,iod.optdtlcode,d.taxReport,d.pkpsts,d.taxAmount,d.docstatus,
d1.quantity*d1.price as jumlah,
d1.quantity*iph.price as jumlahHPDSAIPlusP3,
case when d1.isItemBns='N' then d1.quantity else 0 end * iph.price as jumlahHPDSAI,
(d1.quantity*d1.price)*discprcnt1/100 as p1,
(d1.quantity*d1.price)*discprcnt2/100 as p2,
(d1.quantity*d1.price)*discprcnt3/100 as p3,
(d1.quantity*d1.price)-
((d1.quantity*d1.price)*discprcnt1/100 +
(d1.quantity*d1.price)*discprcnt2/100 +
(d1.quantity*d1.price)*discprcnt3/100 ) as jumlahkotor ,

(select sum((d1.quantity*d1.price)-
((d1.quantity*d1.price)*discprcnt1/100 +
(d1.quantity*d1.price)*discprcnt2/100 +
(d1.quantity*d1.price)*discprcnt3/100 ))
from deliverydetail1 d1 where d1.docentry=d.docentry) 
 as TotJmlKtr,
	d.pricecode,iph.price,d.ref1,d.ref2
 from delivery d
inner join deliverydetail1 d1 on d.docentry=d1.docentry and
	year(d.docdate)=year($P{docdate}) and month(d.docdate)=month($P{docdate}) and d.docdate<=$P{docdate} and
	d1.whscode=case when $P{whscode}='' then d1.whscode else $P{whscode} end and
	d.slPrsnCode=case when rtrim('')='' then d.slPrsnCode else '' end
left join itempricehistory iph on iph.itemcode=d1.itemcode and 
	iph.yop=year($P{docdate}) and iph.mop=month($P{docdate}) and iph.pricecode='HPD'
left join `order` o on o.docentry=d1.baseentry
left join 	itemoptdetail1 iod on iod.ownercode=d1.itemcode and 
		optionalCode=case when $P{optionalCode}='' then '01' else $P{optionalCode} end and 
		iod.optDtlCode between 
		case when $P{optDtlCodeFr}='' then iod.optDtlCode else $P{optDtlCodeFr} end and 
		case when $P{optDtlCodeTo}='' then iod.optDtlCode else $P{optDtlCodeTo} end	
) as x
left join customer c on c.custmrcode=x.custmrcode
left join itemoptionalinfo ioi on ioi.optionalcode=x.optionalcode and x.optdtlcode=ioi.optdtlcode
left join itemoptsett ios on ios.optionalcode=x.optionalcode
group by 	ordno,docnum,custmrcode,x.whsCode,x.TotJmlKtr,x.taxReport,
		x.docdate,x.docduedate,x.slPrsnCode,x.optionalcode,x.pkpsts,x.taxAmount,x.docstatus,
		x.pricecode

) as y
left join salesman s on s.slPrsnCode=y.slPrsnCode]]></queryString>

