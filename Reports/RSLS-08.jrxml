	<parameter name="dateto" isForPrompting="true" class="java.lang.String"/>
	<parameter name="pricecd" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select d.docdate, d.docnum,d.custmrcode, c.custmrname as outlet ,n.npwpnumber, sum(d1.quantity*iph.price) as Bruto, 
sum(d1.quantity*iph.price) as potongan, sum((d1.quantity*iph.price)-(d1.quantity*iph.price)) as JmlGross,
sum((((d1.quantity*iph.price)-((d1.quantity*iph.price)))/d.taxpercent)*10) as ppn, 
sum((d1.quantity*iph.price)-((d1.quantity*iph.price)))-sum((((d1.quantity*iph.price)-((d1.quantity*iph.price)))/d.taxpercent)*10) as netto,
 iod.optdtlcode, iof.dscription as nmbrand, iph.pricecode
from
delivery d inner join deliverydetail1 d1 on 
d.docentry=d1.docentry and d1.isitembns='Y' and d.docdate between $P{datefr} and $P{dateto}  
inner join  itempricehistory iph on d1.itemcode=iph.itemcode and iph.pricecode=$P{pricecd} 
and iph.yop=year(d.docdate) and iph.mop=month(d.docdate) 
left join customer c on d.custmrcode=c.custmrcode 
left join npwp n on c.custmrcode=n.ownercode 
inner join itemoptdetail1 iod on d1.itemcode=iod.ownercode and iod.optionalcode=01  and iod.optdtlcode = case when $P{brand}='' then iod.optdtlcode else $P{brand} end  
inner join itemoptionalinfo iof on iof.optionalcode=iod.optionalcode  and iof.optdtlcode=iod.optdtlcode
group by d.custmrcode]]></queryString>

