	<queryString><![CDATA[select p.cmpnyid,p.cmpnyname,
a.optDtlCode,ioi.dscription as brandname,a.custmrcode,a.slprsncode,s.slprsnname,
ad.dscription, ad.address,
sum(a.invval) as invval,sum(a.rtrval) as rtrval,sum(a.invval)-sum(a.rtrval) as ttlku
from
(
select iod.optDtlCode, s.custmrcode,s.slprsncode,s1.quantity*iph.price as invval,0 as rtrval from
delivery s 
inner join deliverydetail1 s1 on s.docentry=s1.docentry and cast(cast(s.docdate as char(11)) as datetime) 
between cast($P{datefr} as datetime) and cast(cast($P{dateto} as datetime) as datetime) 
		and s1.isitembns='N'
inner join itemoptdetail1 iod on iod.optionalCode='01' and 
iod.optDtlCode=case when $P{brandcode}='' then iod.optDtlCode else  $P{brandcode} end
and iod.ownercode=s1.itemcode
inner join itempricehistory iph on iph.pricecode='HPD' and iph.itemcode=s1.itemcode and
		iph.yop=year(s.docdate) and iph.mop=month(s.docdate)
union all
select iod.optDtlCode,s.custmrcode,s.slprsncode,0 as invval,s1.quantity*iph.price  as rtrval from
creditmemo s 
inner join creditmemodetail1 s1 on s.docentry=s1.docentry and cast(cast(s.docdate as char(11)) as datetime) 
between cast($P{datefr} as datetime) and cast(cast($P{dateto} as datetime) as datetime)
		and s1.isitembns='N'
inner join itemoptdetail1 iod on iod.optionalCode='01' and 
iod.optDtlCode=case when $P{brandcode}='' then iod.optDtlCode else  $P{brandcode} end and iod.ownercode=s1.itemcode 
inner join itempricehistory iph on iph.pricecode='HPD' and iph.itemcode=s1.itemcode and
		iph.yop=year(s.docdate) and iph.mop=month(s.docdate)
) as a
inner join address ad on a.custmrcode = ad.ownercode and ad.addrtypecode='02'
inner join salesman s on s.slprsncode=a.slprsncode
inner join itemoptionalinfo ioi on ioi.optionalcode='01' and ioi.optdtlcode=a.optDtlCode
cross join preference p
group by a.optDtlCode,a.custmrcode,p.cmpnyid,a.slprsncode,s.slprsnname]]></queryString>

