	<queryString><![CDATA[select x.yop,x.mop,x.optdtlcode,x.nmbrand,x.slprsncode,x.slprsnname,x.custmrcode,
x.dscription as namaoutlet,x.docdate,x.noorder, x.NoInvoice,x.QtyOrder,
x.QtyReal, x.ValOrder, x.ValReal
from
(
select iph.yop,iph.mop,iof.optdtlcode,iof.dscription as nmbrand,d.slprsncode,s.slprsnname,d.custmrcode,
a.dscription,d.docdate,dd1.basedocnum as noorder, d.docnum as NoInvoice, sum(od1.quantity) as QtyOrder,
sum(dd1.quantity) as QtyReal, sum(od1.quantity*iph.price) as ValOrder, sum(dd1.quantity*iph.price) as ValReal
from deliverydetail1 dd1 
inner join delivery d on d.docentry=dd1.docentry 
and cast(cast(d.docdate as char(11)) as datetime) between cast($P{datefr} as datetime) and cast($P{dateto} as datetime) 
inner join orderdetail1 od1 on dd1.baseline=od1.linenum and dd1.baseentry=od1.docentry
inner join itemoptdetail1 iod on iod.ownercode=dd1.itemcode and iod.optionalcode='01'
inner join itemoptionalinfo iof on iod.optdtlcode=iof.optdtlcode and iof.optionalcode='01'
inner join salesman s on s.slprsncode=d.slprsncode and d.slprsncode between $P{salesfr} and $P{salesto}
inner join address a on a.ownercode=d.custmrcode and a.addrtypecode='02' 
inner join itempricehistory iph on dd1.itemcode=iph.itemcode and iph.yop=year(d.docdate)
and iph.mop=month(d.docdate) and iph.pricecode=$P{price}
group by iph.yop,iph.mop,iof.optdtlcode,iof.dscription,d.slprsncode,s.slprsnname,d.custmrcode,
a.dscription,d.docdate,dd1.basedocnum, d.docnum
) as x
where x.optdtlcode between $P{optdtlfr} and $P{optdtlto}]]></queryString>

