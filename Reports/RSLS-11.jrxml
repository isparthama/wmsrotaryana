	<queryString><![CDATA[select 
case when $P{sortbyitem}='1' then d1.itemcode else d.custmrcode end as sort1,
case when $P{sortbyitem}='1' then d.custmrcode else d1.itemcode end as sort2, 
p.cmpnyid,p.cmpnyname,d.docdate, d.docnum, d.custmrcode,a.addrcode,a.dscription as nmoutlet,
d1.itemcode, i.dscription as nmbrg,d1.quantity,iod.optdtlcode as brand,
iof.dscription as nmbrand,iph.price, d1.quantity*iph.price as jumlah,iph.pricecode
from
delivery d inner join deliverydetail1 d1 on d.docentry=d1.docentry and d.custmrcode=$P{cstmr} and
d.docdate between cast(cast($P{datefr} as char(11)) as datetime) and cast(cast($P{dateto} as char(11)) as datetime) and d.docstatus<>'B'
inner join address a on a.ownercode=d.custmrcode and a.addrtypecode='02' 
inner join item i on d1.itemcode=i.itemcode
inner join itemoptdetail1 iod on iod.ownercode=d1.itemcode and iod.optionalcode='01'
inner join itemoptionalinfo iof on iof.optionalcode=iod.optionalcode and iof.optdtlcode=iod.optdtlcode and iod.optdtlcode=$P{brnd}
inner join itempricehistory iph on d1.itemcode=iph.itemcode and iph.yop=year(d.docdate) and iph.mop=month(d.docdate)
and iph.pricecode='HPD', preference p]]></queryString>

