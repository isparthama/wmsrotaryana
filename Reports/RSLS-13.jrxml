	<queryString><![CDATA[select p.cmpnyid, p.cmpnyname,
y.brandku, iof.dscription,
sum(y.direct_MT) as direct_MT,sum(y.direct_CT) as direct_CT,sum(y.direct_SD) as direct_SD ,
sum(y.indirect_MT) as indirect_MT,sum(y.indirect_CT) as indirect_CT,sum(y.indirect_SD) as indirect_SD,
sum(y.direct_MTkum) as direct_MTkum, sum(y.direct_CTkum) as direct_CTkum,sum(y.direct_SDkum) as direct_SDkum,
sum(y.indirect_MTkum) as indirect_MTkum,sum(y.indirect_CTkum) as indirect_CTkum,sum(y.indirect_SDkum) as indirect_SDkum
from
	(
	select 
	z.brandku, 
	sum(z.direct_MT) as direct_MT,sum(z.direct_CT) as direct_CT,sum(z.direct_SD) as direct_SD ,
	sum(z.indirect_MT) as indirect_MT,sum(z.indirect_CT) as indirect_CT,sum(z.indirect_SD) as indirect_SD,
	0 as direct_MTkum, 0 as direct_CTkum, 0 as direct_SDkum, 0 as indirect_MTkum, 0 as indirect_CTkum,
	0 as indirect_SDkum
	from
	(
		select a2.brandku,
		n1.custmrcode ,
		case when n1.tradetype='01' and n1.otlttype ='01' then a2.stm else '0' end as direct_MT,
		case when n1.tradetype='01' and n1.otlttype ='02' then a2.stm else '0' end as direct_CT,
		case when n1.tradetype='01' and n1.otlttype='03' then a2.stm else '0' end as direct_SD,
		case when n1.tradetype='02' and n1.otlttype ='01' then a2.stm else '0' end as indirect_MT,
		case when n1.tradetype='02' and n1.otlttype ='02' then a2.stm else '0' end as indirect_CT,
		case when n1.tradetype='02' and n1.otlttype='03' then a2.stm else '0' end as indirect_SD

		from
		(
			select n.custmrcode,max(n.direct) as tradetype,max(n.otlttype) as otlttype
			from
			(
				select ownercode as custmrcode,optdtlcode as direct,'' as otlttype from custoptsettdetail1 where optionalcode='01'
				union all
				select ownercode,'' as direct,optdtlcode as tradetype from custoptsettdetail1 where optionalcode='02'
			) as n
			group by n.custmrcode
		) as n1

		inner join

		(
			/* --------------- stmku ------------------------*/
			select x1.custmrcode,iod.optdtlcode as brandku,sum(x1.STM) as STM
			from
			(
				select x.custmrcode as custmrcode,x.itemcode,sum(x.quantity*x.price) as stm 
				from
				(
				select d.custmrcode,d1.itemcode,d1.quantity as quantity,iph.price as price from deliverydetail1 d1 
				inner join delivery d on d.docentry=d1.docentry and d1.isitembns<>'Y' and 
					d.docstatus<>'B' and cast(cast(d.docdate as char(11)) as datetime) between 
					cast($P{dateTo} as datetime) and cast($P{dateTo} as datetime)
				left join itempricehistory  iph on iph.yop=year($P{dateFr}) and iph.mop=month($P{dateFr}) and 
				iph.pricecode='HPD' and iph.itemcode=d1.itemcode
				) as x
				group by x.custmrcode,x.itemcode
				) as x1
			inner join itemoptdetail1 as iod on iod.ownercode=x1.itemcode and iod.optionalcode='01' 
			group by x1.custmrcode,iod.optdtlcode
			/* --------------- stmku ------------------------*/
		) as a2 on a2.custmrcode=n1.custmrcode
	) as z
	group by z.brandku

	union all


	select z.brandku, 0,0,0,0,0,0,
	sum(z.direct_MT) as direct_MTk,sum(z.direct_CT) as direct_CTk,sum(z.direct_SD) as direct_SDk ,
	sum(z.indirect_MT) as indirect_MTk,sum(z.indirect_CT) as indirect_CTk,sum(z.indirect_SD) as indirect_SDk 
	from
	(
		select a2.brandku,
		n1.custmrcode ,
		case when n1.tradetype='01' and n1.otlttype ='01' then a2.stm else '0' end as direct_MT,
		case when n1.tradetype='01' and n1.otlttype ='02' then a2.stm else '0' end as direct_CT,
		case when n1.tradetype='01' and n1.otlttype='03' then a2.stm else '0' end as direct_SD,
		case when n1.tradetype='02' and n1.otlttype ='01' then a2.stm else '0' end as indirect_MT,
		case when n1.tradetype='02' and n1.otlttype ='02' then a2.stm else '0' end as indirect_CT,
		case when n1.tradetype='02' and n1.otlttype='03' then a2.stm else '0' end as indirect_SD

		from
		(
			select n.custmrcode,max(n.direct) as tradetype,max(n.otlttype) as otlttype
			from
			(
				select ownercode as custmrcode,optdtlcode as direct,'' as otlttype from custoptsettdetail1 where optionalcode='01'
				union all
				select ownercode,'' as direct,optdtlcode as tradetype from custoptsettdetail1 where optionalcode='02'
			) as n
			group by n.custmrcode
		) as n1

		inner join

		(
			/* --------------- stmku ------------------------*/
			select x1.custmrcode,iod.optdtlcode as brandku,sum(x1.STM) as STM
			from
			(
				select x.custmrcode as custmrcode,x.itemcode,sum(x.quantity*x.price) as stm 
				from
				(
				select d.custmrcode,d1.itemcode,d1.quantity as quantity,iph.price as price from deliverydetail1 d1 
				inner join delivery d on d.docentry=d1.docentry and d1.isitembns<>'Y' and 
					d.docstatus<>'B' and cast(cast(d.docdate as char(11)) as datetime) between 
					cast($P{dateFr} as datetime) and cast($P{dateTo} as datetime)
				left join itempricehistory  iph on iph.yop=year($P{dateFr}) and iph.mop=month($P{dateFr}) and 
				iph.pricecode='HPD' and iph.itemcode=d1.itemcode
				) as x
				group by x.custmrcode,x.itemcode
				) as x1
			inner join itemoptdetail1 as iod on iod.ownercode=x1.itemcode and iod.optionalcode='01'
			group by x1.custmrcode,iod.optdtlcode
			/* --------------- stmku ------------------------*/
		) as a2 on a2.custmrcode=n1.custmrcode
	) as z
	group by z.brandku
	) as y
inner join itemoptionalinfo iof on iof.optionalcode='01' and iof.optdtlcode=y.brandku,
preference p
group by y.brandku]]></queryString>

