	<queryString><![CDATA[select d.docdate,d.slprsncode, sl.slprsnname, d.custmrcode, a.dscription as namaoutlet, a.address, iof.optdtlcode as brandcode, iof.dscription as brandname, sum(d1.quantity * iph.price) as penjualan  
from
deliverydetail1 d1 inner join delivery d on d1.docentry=d.docentry and d.docstatus<>'B' and d1.isitembns='N' and d.docdate between $P{dateFr} and $P{dateTo} and case when $P{whscode}='' then d1.whscode else $P{whscode} end =d1.whscode and d.slprsncode between 
case when $P{slprsnCodeFr}='' then 
(select slprsncode from salesman order by slprsncode asc limit 1)
else $P{slprsnCodeFr} end
and 
case when $P{slprsnCodeTo}='' then 
(select slprsncode from salesman order by slprsncode desc limit 1)
else $P{slprsnCodeTo} end
inner join itempricehistory iph on iph.itemcode=d1.itemcode and iph.yop=year(d.docdate) and iph.mop=month(d.docdate) and iph.pricecode='HPD'
inner join itemoptdetail1 iod on iod.ownercode=d1.itemcode and iod.optionalcode='01'
inner join itemoptionalinfo iof on iof.optionalcode='01' and iof.optdtlcode=iod.optdtlcode
inner join warehouse w on w.whscode=d1.whscode and case when $P{whscode}='' then w.whscode else $P{whscode} end = w.whscode
inner join salesman sl on sl.slprsncode=d.slprsncode
inner join address a on a.ownercode = d.custmrcode and a.addrtypecode='02'
group by d.slprsncode, sl.slprsnname, d.custmrcode, a.dscription, a.address, iof.optdtlcode, iof.dscription]]></queryString>

