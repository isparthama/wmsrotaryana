	<queryString><![CDATA[select cmpnyname,ref2,docnum,docdate,b.bpitem,x.itemcode,dscription,x.custmrcode,custmrname,quantity
from (
select p.cmpnyname,o.ref2,d.docnum,d.docdate,dd.itemcode,i.dscription,concat(d.custmrcode,d.shipToCode) as custmrcode,a.dscription as custmrname,dd.quantity
from delivery d
inner join deliverydetail1 dd on d.docentry=dd.docentry
inner join address a on a.ownerCode=d.custmrCode and a.addrCode=d.shipToCode and addrTypeCode='02' and a.objType=13
inner join item i on i.itemCode=dd.itemcode
inner join `order` o on o.docnum=dd.baseDocNum
cross join preference p
where d.docdate=$P{datefr} and dd.quantity>0
) as x
inner join bpitem b on b.itemcode=x.itemcode and b.custmrCode='00SMB00'
order by docnum,bpitem]]></queryString>

