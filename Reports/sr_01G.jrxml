	<queryString><![CDATA[SELECT a.suppName,a.cmpnyName,a.address,
a.docnum,a.docdate,a.docstatus,a.dscription,a.reffno,a.refftype,
a.itemname,
a.racknofrm,
a.itemcode,a.univcode,a.whscode,a.whsname,
a.lockdesc,a.racktypecode,
a.racktypedesc,a.pathno,a.rackno,a.putno,
a.pltqty,a.inrqty,a.pcsqty,a.ttlpcsqty ,a.barangku,ifnull(a.rr,'-') rr,a.isiPallet,
ifnull(b.batchno,'-') as batchno ,b.expiredate as expiredate,b.pltqty2,b.inrqty2,b.pcsqty2,b.ttlpcsqty2 ,
c.isCheckBatchNo
FROM
(
SELECT a.docentry,CONCAT(sup.suppname,'/',isup.suppcode) AS suppName,p.cmpnyName,p.address,
a.docnum,a.docdate,a.docstatus,a.dscription,a.reffno,a.refftype,
a.itemname,
CONCAT(MID(a.rackno,1,3),"-",MID(a.rackno,4,2),'-',MID(a.rackno,6,2),'-',MID(a.rackno,8,2)) AS racknofrm,
a.itemcode,a.univcode,a.whscode,CONCAT(w.dscription,'/',a.whscode) AS whsname,
a.lockdesc,a.racktypecode,
a.racktypedesc,a.pathno,a.rackno,a.putno,
SUM(a.pltqty) AS pltqty,SUM(a.inrqty) AS inrqty,SUM(a.pcsqty) AS pcsqty,SUM(a.ttlpcsqty) AS ttlpcsqty ,
a.barangku,a.rr,a.isiPallet
FROM
(
SELECT t.docentry,t.docnum,t.docdate,t.docstatus,t.dscription,t.reffno,t.refftype,
itm.dscription AS itemname,
t1.itemcode,bp.bpitem AS univcode,t1.whscode,l.dscription AS lockdesc,l.racktypecode,
r.dscription AS racktypedesc,l.pathno,t1.rackno,l.putno,
t1.pltqty,t1.inrqty,t1.pcsqty,t1.ttlpcsqty ,CONCAT(t1.pltqty,'/',t1.ttlpcsqty/t1.uom2) AS isiPallet,
CASE WHEN $P{univcode}='1' THEN CONCAT(RTRIM(bp.bpitem),"  ",
RTRIM(itm.dscription)) ELSE CONCAT(RTRIM(t1.itemcode),
"  ",RTRIM(itm.dscription)) END AS barangku,si.ref2 AS rr
FROM sr_transin t
INNER JOIN sr_transin_print tp ON t.docnum=tp.docnum
LEFT JOIN sr_transindetail1 t1 ON t.docentry=t1.docentry
LEFT JOIN sr_locate l ON l.rackno=t1.rackno AND l.whscode=t1.whscode
LEFT JOIN item itm ON itm.itemcode=t1.itemcode
LEFT JOIN sr_racktype r ON r.sr_racktypecode=l.racktypecode
LEFT JOIN bpitem bp ON bp.itemcode=t1.itemcode AND bp.custmrcode='00SMB00'
LEFT JOIN goodsreceipt si ON si.docentry=t1.baseentry AND t1.basetype=36
WHERE tp.audituser = $P{audituser}
) AS a 
LEFT JOIN itemsupplier isup ON isup.itemcode=a.itemcode AND isup.priority=1 AND isup.active='Y'
LEFT JOIN supplier sup ON sup.suppcode=isup.suppcode
LEFT JOIN warehouse w ON w.whscode=a.whscode
, preference p
GROUP BY 
a.docentry,a.docnum,a.docdate,a.docstatus,a.dscription,a.reffno,a.refftype,
a.itemname,
a.itemcode,a.univcode,a.whscode,a.lockdesc,a.racktypecode,
a.racktypedesc,a.pathno,a.rackno,a.putno,
a.barangku,a.rr,a.isiPallet
) AS a
LEFT JOIN ( SELECT docentry,whscode,rackno,itemcode,batchno,expiredate,
	SUM(pltqty) AS pltqty2,SUM(inrqty) AS inrqty2,SUM(pcsqty) AS pcsqty2,SUM(ttlpcsqty) AS ttlpcsqty2 FROM sr_transindetail2
	GROUP BY docentry,whscode,rackno,itemcode,batchno,expiredate)
AS b ON a.docentry=b.docentry AND a.whscode=b.whscode AND a.rackno=b.rackno AND a.itemcode=b.itemcode
	,sr_option c
ORDER BY a.pathno ASC,a.itemcode asc]]></queryString>

