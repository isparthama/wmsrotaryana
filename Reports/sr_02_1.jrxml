	<queryString><![CDATA[select @no:=@no+1 as a,mod(@no,4) as no2,
t.docnum,t.docdate,t.docstatus,t.dscription,t.reffno,t.refftype,
t.itemname,
t.itemcode,t.univcode,t.whscode,t.lockdesc,
t.racktypecode,t.racktypedesc,t.pathno,t.rackno,
t.pltqty,t.inrqty,t.pcsqty,t.ttlpcsqty ,
t.inCommit,
t.barangku,
t.custmrcode,t.ref2,t.cmpnyidFr,t.areaname,t.Counter,t.tradename
from
(
select t.ref2,
t.docnum,t.docdate,t.docstatus,t.dscription,t.reffno,t.refftype,
t.itemname,
t.itemcode,t.univcode,t.whscode,t.lockdesc,
t.racktypecode,t.racktypedesc,t.pathno,t.rackno,
sum(t.pltqty) as pltqty,sum(t.inrqty) as inrqty,sum(t.pcsqty) as pcsqty,sum(t.ttlpcsqty) as ttlpcsqty ,
t.inCommit,
t.barangku,
t.custmrcode,
case when t.cmpnyidFr='01' then 'MB' else 
case when t.cmpnyidFr='02' then 'SAI' else 'OTH' end end as cmpnyidFr ,t.areaname,t.Counter,t.tradename
from
(
select d.ref2,t.docnum,t.docdate,t.docstatus,t.dscription,t.reffno,t.refftype,
itm.dscription as itemname,
t1.itemcode,bp.bpitem as univcode,t1.whscode,l.dscription as lockdesc,
l.racktypecode,r.dscription as racktypedesc,l.pathno,t1.rackno,
t1.pltqty,t1.inrqty,t1.pcsqty,t1.ttlpcsqty ,case when t1.incommit='Y' then '' else '*' end as inCommit,
case when $P{univcode}='1' then 
concat(rtrim(t1.rackno),"  ",rtrim(bp.bpitem),"  ",rtrim(itm.dscription)) else concat(rtrim(t1.rackno),"  ",rtrim(t1.itemcode),"  ",rtrim(itm.dscription)) end as barangku,
case when t.refftype='Sales Confirmation' then concat(rtrim(a.dscription),'#',rtrim(a.address),'#',d.custmrcode) else '' end as custmrcode,left(d.custmrcode,2) as cmpnyidFr,
coi.dscription as areaname,cd.catgrycode as Counter,coi2.dscription as tradename

from sr_transout t
inner join sr_transout_print sto on sto.docnum=t.docnum
left join delivery d on d.docnum=reffno
left join address a on a.ownercode=d.custmrcode and a.addrcode=d.shiptocode and a.addrTypeCode='02'
left join sr_transoutdetail1 t1 on t.docentry=t1.docentry
left join sr_locate l on l.rackno=t1.rackno and l.whscode=t1.whscode
left join item itm on itm.itemcode=t1.itemcode
left join sr_racktype r on r.sr_racktypecode=l.racktypecode
left join bpitem bp on bp.itemcode=t1.itemcode and bp.custmrcode='00SMB00'
left join custoptsettdetail1 cod on cod.ownercode=d.custmrcode and cod.optionalcode='00'
left join sr_counterdetail cd on cd.optionalcode=cod.optionalcode and cd.optdtlcode=cod.optdtlcode
left join custoptinfo coi on coi.optionalcode=cod.optionalcode and coi.optdtlcode=cod.optdtlcode
left join custoptsettdetail1 cod2 on cod2.ownercode=d.custmrcode and cod2.optionalcode='02'
left join custoptinfo coi2 on coi2.optionalcode=cod2.optionalcode and coi2.optdtlcode=cod2.optdtlcode
where sto.audituser= $P{audituser}
) as t
group by t.t.ref2,
t.docnum,t.docdate,t.docstatus,t.dscription,t.reffno,t.refftype,
t.itemname,
t.itemcode,t.univcode,t.whscode,t.lockdesc,
t.racktypecode,t.racktypedesc,t.pathno,t.rackno,
t.inCommit,t.barangku,t.custmrcode,t.cmpnyidFr,t.areaname,t.Counter,t.tradename
) as t,(select @no:=0 ) as a
order by t.docnum ,t.racktypecode,t.pathno]]></queryString>

