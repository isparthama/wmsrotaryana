	<queryString><![CDATA[select
a.docnum,a.docdate,a.dscription,a.reffNo,a.refftype ,
a.whscode,a.rackno,bp.bpitem,a.itemcode,i.dscription as itemname,
a.inrqty,a.pcsqty,a.ttlpcsqty,a.pkali,a.visorder,a.visorder2
from
(
select
0 as docnum,now() as docdate,'Saldo Awal' as dscription,0 as reffNo,
'Saldo Awal'  as refftype ,a.whscode,a.rackno,a.itemcode,
a.innercount as inrqty,
a.pieces as pcsqty,
a.piecesamt as ttlpcsqty,1 as pkali,0 as visorder,1 as visorder2
from
(
select
yop,mop,whscode,rackno,itemcode,
sum(innercount) as innercount,sum(pieces) as pieces,sum(piecesamt) as piecesamt
from
(
select year($P{transdate}) as yop,month($P{transdate}) as mop,whscode,rackno,
itemcode,0 as innercount,0 as pieces,0 as piecesamt
from 
( select * from sr_itemlocate where itemcode=$P{itemcode} and whscode=$P{whscode}) as a
union all
select yop,mop,whscode,rackno,itemcode,innercount,pieces,piecesamt
from sr_rackbalance where itemcode=$P{itemcode} and whscode=$P{whscode} and
	year($P{transdate})=yop and month($P{transdate})=mop
) as a
group by yop,mop,whscode,rackno,itemcode
) as a
union all 
select 
t.docnum,t.docdate,t.dscription,t.reffNo,t.refftype ,
t1.whscode,t1.rackno,t1.itemcode,t1.inrqty,t1.pcsqty,t1.ttlpcsqty,1 as pkali,1 as visorder,2 as visorder2
from sr_transin t inner join sr_transindetail1 t1 on t.docentry=t1.docentry
where year(t.docdate)=year($P{transdate}) and month(t.docdate)=month($P{transdate}) and t1.itemcode=$P{itemcode} 
and t1.whscode=$P{whscode} 
union all
select 
t.docnum,t.docdate,t.dscription,t.reffNo,t.refftype ,
t1.whscode,t1.rackno,t1.itemcode,t1.inrqty,t1.pcsqty,t1.ttlpcsqty,-1 as pkali,2 as visorder,2 as visorder2
from sr_transout t inner join sr_transoutdetail1 t1 on t.docentry=t1.docentry
where year(t.docdate)=year($P{transdate}) and month(t.docdate)=month($P{transdate}) and t1.itemcode=$P{itemcode} 
and t1.whscode=$P{whscode} 
) as a
left join item i on i.itemcode=a.itemcode
left join bpitem bp on bp.itemcode=a.itemcode and bp.custmrcode='00SMB00']]></queryString>

