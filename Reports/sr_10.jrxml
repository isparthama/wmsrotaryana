	<queryString><![CDATA[select @no:=@no+1 as no,tr.docnum,tr.docdate,tr.docstatus,tr.refftype,tr.docnumout as transout,docnumin as transin 
,cast($P{docdatefr} as datetime) as datefr,cast($P{docdateto} as datetime) as dateto,
concat('Item By ',case when $P{itemcode}='' then 'All Item' else $P{itemcode} end) as txtItemBy,
concat('Doc Status By ',case when $P{docstatus}='' then 'All Status' else $P{docstatus} end) as txtDocStatusBy
from 
sr_transtransfer tr  ,(select @no:=0) as no
where tr.docnum between 
	case when $P{docnumfr}='' then tr.docnum else $P{docnumfr} end and
	case when $P{docnumto}='' then tr.docnum else $P{docnumto} end 
and exists
( select * from sr_transout sto inner join sr_transoutdetail1 sto1 
on sto.docentry=sto1.docentry and sto1.whscode=$P{whscode} and
sto1.itemcode=
case when $P{itemcode}='' then sto1.itemcode  else $P{itemcode} end and sto.refftype='Rack Transfer' 
where 	sto.docnum=tr.docnumout ) and 
tr.docstatus=case when $P{docstatus}='' then tr.docstatus else $P{docstatus} end and
cast(cast(tr.docdate as char(11)) as datetime) between cast($P{docdatefr} as datetime) and cast($P{docdateto} as datetime);]]></queryString>

