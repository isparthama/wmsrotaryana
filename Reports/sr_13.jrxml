	<queryString><![CDATA[SELECT 
@NO:=@NO+1 AS no,CASE WHEN $P{doublepick}='Y' THEN 'Document Double Pick' ELSE 'All Document Pick' END AS txtdoublepick,
CASE WHEN tr.dscription='MT' THEN ttlpcsqty ELSE 0 END AS mtqty,
CASE WHEN tr.dscription='CT' THEN ttlpcsqty ELSE 0 END AS ctqty,
CASE WHEN tr.dscription='OTH' THEN ttlpcsqty ELSE 0 END AS othqty,
CASE WHEN tr.dscription='MT' THEN 1 ELSE 0 END AS cmtqty,
CASE WHEN tr.dscription='CT' THEN 1 ELSE 0 END AS cctqty,
CASE WHEN tr.dscription='OTH' THEN 1 ELSE 0 END AS cothqty,
tr.optdtlcode,tr.dscription ,tr.custmrcode,tr.reffno,tr.audituser,tr.docnum,tr.docdate,
CONCAT(DAY(tr.docdate),'/',MONTH(tr.docdate)) AS docdatefrm,tr.docstatus,tr.refftype,0 AS transout,0 AS transin 
,CAST($P{docdatefr} AS DATETIME) AS datefr,CAST($P{docdateto} AS DATETIME) AS dateto,
CONCAT('Item By ',CASE WHEN $P{itemcode}='' THEN 'All Item' ELSE $P{itemcode} END) AS txtItemBy,
CONCAT('Doc Status By ',CASE WHEN $P{docstatus}='' THEN 'All Status' ELSE 
CASE WHEN $P{docstatus}='O' THEN 'Open' ELSE 'Close' END END) AS txtDocStatusBy,auditdate,sortby,dblpick,
regdate,registerdate,ttlpcsqty,CONCAT(DAY(auditdate),'/',MONTH(auditdate)) AS auditdatefrm
FROM
(
SELECT DISTINCT 
CASE WHEN x.reffno IS NULL THEN 0 ELSE 1 END AS dblpick,
cod.optdtlcode,coi.dscription ,d.custmrcode,a.audituser,a.docnum,a.docdate,a.docstatus,a.reffno,CONCAT('D/N: ',a.reffno,' ',REPLACE(a.dscription,a.reffno,'')) AS refftype ,
a.auditdate,CASE WHEN $P{groupbytrade}='Y' THEN cod.optdtlcode ELSE a.docnum END AS sortby,
CONCAT(DAY(a.registerdate),'/',MONTH(a.registerdate)) AS regdate,a.registerdate,a.ttlpcsqty
FROM 
(SELECT 
a.docnum,a.docdate,a.docstatus,a.reffno,a.dscription,a.auditdate,a.registerdate,a.audituser,
a1.baseentry,
SUM(a1.ttlpcsqty) AS ttlpcsqty
FROM sr_transout a 
INNER JOIN sr_transoutdetail1 a1 ON a.docentry=a1.docentry AND a1.basetype=28
AND a.docdate >= DATE_SUB(DATE(NOW()), INTERVAL 3 MONTH)
	AND
	a1.whscode=$P{whscode} AND 
	CAST(CAST(a.docdate AS CHAR(11)) AS DATETIME) BETWEEN
		CASE WHEN $P{docdatefr}='' THEN CAST(CAST(a.docdate AS CHAR(11)) AS DATETIME) ELSE CAST($P{docdatefr} AS DATETIME) END AND 
		CASE WHEN $P{docdateto}='' THEN CAST(CAST(a.docdate AS CHAR(11)) AS DATETIME) ELSE CAST($P{docdateto} AS DATETIME) END AND 
	a1.itemcode=	
		CASE WHEN $P{itemcode}='' THEN a1.itemcode ELSE $P{itemcode} END AND
	a.docnum BETWEEN 
		CASE WHEN $P{docnumfr}='' THEN a.docnum ELSE $P{docnumfr} END AND 
		CASE WHEN $P{docnumto}='' THEN a.docnum ELSE $P{docnumto} END AND
	a.docstatus=CASE WHEN $P{docstatus}='' THEN a.docstatus ELSE $P{docstatus} END
GROUP BY a.docnum,a.docdate,a.docstatus,a.docstatus,a.reffno,a.dscription,a.auditdate,a.registerdate,a.audituser,a1.baseentry
) AS a
INNER JOIN delivery d ON d.docentry=a.baseentry  
LEFT JOIN custoptsettdetail1 cod ON cod.ownercode=d.custmrcode AND cod.optionalcode='02'
LEFT JOIN custoptinfo coi ON coi.optionalcode=cod.optionalcode AND coi.optdtlcode=cod.optdtlcode
LEFT JOIN (
SELECT reffno,COUNT(reffno) AS a FROM sr_transout WHERE refftype='Sales Confirmation' 
	AND docdate >= DATE_SUB(DATE(NOW()), INTERVAL 3 MONTH)
	AND docdate 
	BETWEEN 
	CASE WHEN $P{docdatefr}='' THEN CAST(CAST(docdate AS CHAR(11)) AS DATETIME) ELSE CAST($P{docdatefr} AS DATETIME) END AND 
		CASE WHEN $P{docdateto}='' THEN CAST(CAST(docdate AS CHAR(11)) AS DATETIME) ELSE CAST($P{docdateto} AS DATETIME) END 
GROUP BY reffno
HAVING COUNT(reffno)>1) AS x ON x.reffno=a.reffno
ORDER BY CASE WHEN $P{groupbytrade}='Y' THEN cod.optdtlcode ELSE a.docnum END ,a.docnum
) AS tr, (SELECT @NO:=0 ) brs
WHERE dblpick=CASE WHEN $P{doublepick}='Y' THEN '1' ELSE dblpick end]]></queryString>

