	<queryString><![CDATA[select 	a.suppcode as owner,a.yop,a.mop,a.racksts,a.docnum,a.docdate,sto.docnum as pickno,sto.docdate as pickdate,
	iod.optdtlcode as brandcode,a.itemcode,i.dscription as itemname ,sum(a.baseQty) as orderqty,sum(a.quantity) as realqty , 
	sum(a.baseQty) -sum(a.quantity) as unRealqty
from
(
select 	ip.suppcode,year(a.docdate) as yop,month(a.docdate) as mop,a.racksts,
	a.docnum,a.docdate,
	a1.itemcode,a1.baseQty,a1.quantity 
from deliverydetail1 a1 inner join delivery a on a.docentry=a1.docentry
left join itemsupplier ip on ip.itemcode=a1.itemcode and ip.active='Y' and ip.priority=1
where year(a.docdate)=$P{yop} and month(a.docdate)=$P{mop} and   
a.racksts=if($P{status}='A',a.racksts,$P{status})
) as a
left join sr_transout sto on sto.reffno=a.docnum and sto.refftype='Sales Confirmation'
left join item i on a.itemcode=i.itemcode
left join itemoptdetail1 iod on iod.optionalcode='01' and iod.ownercode=a.itemcode
where a.suppcode=if($P{owner}='',a.suppcode,$P{owner}) 
group by  a.suppcode,a.yop,a.mop,a.docnum,a.docdate,a.racksts,
	  a.itemcode;]]></queryString>

