	<queryString><![CDATA[SELECT a1.whscode,b.skey,a.docnum,a.docdate,a.ref1,a.ref2,a.comments,COUNT(a1.itemcode) AS itemqty,SUM(a1.quantity) AS ttlqty FROM pst.purchasereceive a
INNER JOIN pst.purchasereceivedetail1 a1 ON a.docentry=a1.docentry AND  YEAR(a.docdate)=$P{yop} AND MONTH(a.docdate)=$P{mop} AND a1.whscode=$P{whscode},
(
SELECT skey,COUNT(skey) keyno FROM
(
SELECT docnum skey,docdate,docnum FROM pst.purchasereceive WHERE YEAR(docdate)=$P{yop} AND MONTH(docdate)=$P{mop} AND whscode=$P{whscode}
UNION ALL
SELECT ref1 skey,docdate,docnum FROM pst.purchasereceive WHERE YEAR(docdate)=$P{yop} AND MONTH(docdate)=$P{mop} AND whscode=$P{whscode}
) AS a
WHERE skey<>''
GROUP BY skey
HAVING COUNT(skey)>1
) AS b 
WHERE a.docnum=b.skey OR a.ref1=b.skey 
GROUP BY a1.whscode,b.skey,a.docnum,a.docdate,a.ref1,a.ref2,a.comments
ORDER BY skey,docnum;]]></queryString>

