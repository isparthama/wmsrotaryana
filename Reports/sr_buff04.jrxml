	<parameter name="mop" isForPrompting="true" class="java.lang.String"/>
	<parameter name="whscode" isForPrompting="true" class="java.lang.String"/>
	<parameter name="audituser" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  a.ref2,a.docnum,a.docdate,a.custmrcode,a.shipToCode,
	a.auditdate,a.audituser,a1.whscode,SUM(a1.quantity) AS ttlqty
	FROM mb.goodsreceipt a INNER JOIN mb.goodsreceiptdetail1 a1 ON a.docentry=a1.docentry AND a1.whsCode=$P{whscode}
	,
	(
	SELECT a.ref2,COUNT(DISTINCT a.ref2) AS n FROM mb.goodsreceipt a 
	WHERE YEAR(a.docDate)=$P{yop} AND MONTH(a.docdate) =$P{mop} AND a.ref2<>''
	GROUP BY a.ref2
	HAVING COUNT(a.ref2)>1
	) AS c 
	WHERE c.ref2=a.ref2 AND YEAR(a.docDate)=$P{yop} AND MONTH(a.docdate) =$P{mop} AND a.ref2<>''
	GROUP BY a.ref2,a.docnum,a.docdate,a.custmrcode,a.shipToCode,
	a.auditdate,a.audituser,a1.whscode
	ORDER BY a.ref2,a.docnum;]]></queryString>

